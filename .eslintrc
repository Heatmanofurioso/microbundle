{
  "extends": [
    "eslint-config-developit",
    "prettier",
    "plugin:prettier/recommended"
  ],
  "parser": "@babel/eslint-parser",
  "plugins": [
    "prettier"
  ],
  "rules": {
    "brace-style": "off",
    "comma-dangle": ["warn", "always-multiline"],
    "indent": "off",
    "require-atomic-updates": "off"
  },
  "settings": {
  },
  "ignorePatterns": [
    "dist"
  ]
}
